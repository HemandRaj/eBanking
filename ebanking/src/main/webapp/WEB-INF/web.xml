<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>ebanking</display-name>
  <servlet>
		<display-name>EmployeeAccount</display-name>
		<servlet-name>EmployeeAccount</servlet-name>
		<servlet-class>com.hemand.ebanking.controller.EmployeeAccount</servlet-class>
	</servlet>
	<servlet>
		<display-name>UserAccount</display-name>
		<servlet-name>UserAccount</servlet-name>
		<servlet-class>com.hemand.ebanking.controller.UserAccount</servlet-class>
	</servlet>
	<servlet>
		<display-name>UserEBanking</display-name>
		<servlet-name>UserEBanking</servlet-name>
		<servlet-class>com.hemand.ebanking.controller.UserEBanking</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EmployeeAccount</servlet-name>
		<url-pattern>/EmployeeAccount</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UserAccount</servlet-name>
		<url-pattern>/jsp/UserAccount</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UserEBanking</servlet-name>
		<url-pattern>/UserEBanking</url-pattern>
	</servlet-mapping>
  <welcome-file-list>
    <welcome-file>jsp/index.jsp</welcome-file>
  </welcome-file-list>
</web-app>